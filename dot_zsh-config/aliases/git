alias soft='git reset --soft HEAD~1 && git reset HEAD .'
alias hard='git reset --hard HEAD~1'
alias wip='git add -A && git commit -m "WIP" --no-verify'
alias gk='gitk --all &'
alias gitbad='git branch --merged | grep -v "\*" | grep -v "master" | grep -v "develop" | xargs -n 1 git branch -d'
alias prsquash='git reset --soft $(git merge-base develop $(git rev-parse --abbrev-ref HEAD))'
